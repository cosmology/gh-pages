{
  "name": "covalent-quickstart",
  "version": "1.0.0",
  "private": true,
  "description": "Forex Market Demo App with @covalent packages",
  "keywords": [
    "angular",
    "components",
    "reusable"
  ],
  "scripts": {
    "start": "node server.js",
    "e2e": "ng e2e",
    "lint": "ng lint",
    "reinstall": "rm -rf node_modules tmp deploy dist && yarn install",
    "postinstall": "webdriver-manager update && ./scripts/protractor.sh",
    "json-server": "json-server -p 4000 db.json && json-server --watch db.json",
    "start-api": "cd mock-api && covalent-data",
    "stop-api": "pkill covalent-data",
    "webdriver-update": "bash ./node_modules/.bin/webdriver-manager update",
    "test": "ng test --code-coverage --single-run",
    "test:watch": "ng test --code-coverage",
    "ghpages-deploy": "ng build --base-href // --aot -prod --sourcemap=false && bash scripts/ghpages-deploy"
  },
  "engines": {
    "node": ">4.4 < 7",
    "npm": ">3"
  },
  "repository": "https://github.com/cosmology/gh-pages.git",
  "license": "MIT",
  "author": "Teradata UX team and Ivan Prokic",
  "contributors": [
    "Kyle Ledbetter <kyle.ledbetter@teradata.com>",
    "Ivan Prokic <ivanprokic@gmail.com>"
  ],
  "dependencies": {
    "@angular/animations": "4.4.3",
    "@angular/cdk": "2.0.0-beta.12",
    "@angular/cli": "1.4.2",
    "@angular/common": "4.4.3",
    "@angular/compiler": "4.4.3",
    "@angular/compiler-cli": "4.4.3",
    "@angular/core": "4.4.3",
    "@angular/flex-layout": "2.0.0-beta.10",
    "@angular/forms": "4.3.0",
    "@angular/http": "4.3.0",
    "@angular/material": "2.0.0-beta.12",
    "@angular/platform-browser": "4.4.3",
    "@angular/platform-browser-dynamic": "4.4.3",
    "@angular/platform-server": "4.4.3",
    "@angular/router": "4.4.3",
    "@covalent/core": "1.0.0-beta.8-1",
    "@covalent/dynamic-forms": "1.0.0-beta.8-1",
    "@covalent/highlight": "1.0.0-beta.8-1",
    "@covalent/http": "1.0.0-beta.8-1",
    "@covalent/markdown": "1.0.0-beta.8-1",
    "@swimlane/ngx-charts": "^5.3.0",
    "core-js": "^2.4.1",
    "d3": "^4.9.0",
    "express": "^4.16.2",
    "hammerjs": "^2.0.8",
    "highlight.js": "9.10.0",
    "json-server": "^0.12.1",
    "moment": "^2.20.1",
    "rxjs": "^5.5.2",
    "showdown": "1.6.4",
    "typescript": "2.3.2",
    "uuid": "^3.1.0",
    "zone.js": "^0.8.5"
  },
  "devDependencies": {
    "@types/hammerjs": "2.0.30",
    "@types/jasmine": "2.5.38",
    "@types/node": "~6.0.60",
    "@types/selenium-webdriver": "2.53.36",
    "codelyzer": "~3.0.0",
    "gh-pages": "^1.1.0",
    "jasmine-core": "~2.5.2",
    "jasmine-spec-reporter": "3.2.0",
    "karma": "~1.4.1",
    "karma-chrome-launcher": "~2.0.0",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^0.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-istanbul": "^0.6.0",
    "karma-spec-reporter": "^0.0.31",
    "phantomjs-prebuilt": "2.1.14",
    "protractor": "~5.1.0",
    "ts-node": "~2.1.0",
    "tslint": "^5.2.0"
  },
  "optionalDependencies": {
    "@covalent/data": "0.7.0"
  }
}
